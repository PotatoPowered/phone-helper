{
    "name": "potatopowered/phone-helper",
    "type": "cakephp-plugin",
    "description": "A CakePHP 3.x telephone number formatting plugin.",
    "keywords": ["phone","telephone","number","helper"],
    "license": "MIT",
    "authors": [
        {
            "name": "Blake Sutton",
            "email": "sutton.blake@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "sphpdox/sphpdox": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "cakephp/cakephp-codesniffer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PhoneHelper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhoneHelper\\Test\\": "tests"
        }
    }
}
